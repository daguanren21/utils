{
  "name": "@daguanren21/utils",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "pnpm@9.12.3",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @daguanren21",
  "author": "daguanren21",
  "license": "MIT",
  "homepage": "https://github.com/daguanren21/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daguanren21/utils.git"
  },
  "bugs": {
    "url": "https://github.com/daguanren21/utils/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@antfu/ni": "^0.23.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^22.10.1",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "esno": "^4.8.0",
    "p-limit": "^6.1.0",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "throttle-debounce": "5.0.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
